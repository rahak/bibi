{
  "name": "pegasus",
  "version": "0.8.7",
  "author": {
    "name": "Satoru MATSUSHIMA"
  },
  "description": "Bibi | EPUB Reader on your website.",
  "keywords": [
    "EPUB",
    "Reading System",
    "Viewer",
    "Books-in-Browsers"
  ],
  "license": "MIT",
  "homepage": "https://bibi.epub.link",
  "repository": {
    "type": "git",
    "url": "git://github.com/satorumurmur/bibi.git"
  },
  "bugs": "https://github.com/satorumurmur/bibi/issues",
  "readmeFilename": "README.md",
  "scripts": {
    "build": "gulp clean:distribution && webpack --config webpack.config.js --progress --colors false --mode production",
    "build+watch": "gulp clean:distribution && webpack --config webpack.config.js --progress --colors --mode production --watch",
    "develop": "gulp clean:distribution && webpack --config webpack.config.js --progress --colors --mode development",
    "develop+watch": "gulp clean:distribution && webpack --config webpack.config.js --progress --colors --mode development --watch",
    "make:distribution": "npm run build && gulp make:distribution",
    "make:dress-template": "gulp make:dress-template",
    "make:lszl": "cd packages/lszl && rm -rf dist && npm i && npm run build --production && cp dist/*.js ../../dev-bib/i/extensions/pegasus",
    "serve": "browser-sync start --config bs-config.js",
    "start": "npm run develop+watch",
    "release": "npm run make:distribution && npx release-it --ci"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "autoprefixer": "^9.7.0",
    "babel-loader": "^8.0.6",
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.3.5",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "file-loader": "^4.2.0",
    "gulp": "^4.0.2",
    "gulp-zip": "^5.0.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.21",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.0",
    "string-replace-webpack-plugin": "^0.1.3",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.2.1",
    "url-loader": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-fix-style-only-entries": "^0.4.0"
  },
  "dependencies": {
    "classlist-polyfill": "^1.2.0",
    "current-script-polyfill": "^1.0.0",
    "custom-event-polyfill": "^1.0.7",
    "easing-js": "^1.1.2",
    "intersection-observer": "^0.7.0",
    "js-yaml": "^3.13.1",
    "jszip": "^3.2.2",
    "jszip-utils": "0.1.0",
    "lsdd": "file:dev-bib/i/extensions/pegasus/lsdd-0.9.4.tgz",
    "lsld": "file:dev-bib/i/extensions/pegasus/lsld-0.6.2+debug.tgz",
    "material-icons": "^0.3.1",
    "native-promise-only": "^0.8.1",
    "polyfill-array-includes": "^2.0.0",
    "sml.js": "^1.0.22",
    "string.padstart": "^0.1.1",
    "text-encoding-utf-8": "^1.0.2",
    "url-polyfill": "^1.1.7"
  },
  "private": true,
  "bundledDependencies": [
    "lsld",
    "lsdd"
  ]
}
